{
  "parserOptions": {
  "ecmaVersion": 6,
  "sourceType": "module",
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true,
    "impliedStrict": true
  }
  },
    "env": {
        "es6": true,
        "node": true,
        "mocha": true,
        "protractor": true,
        "jasmine": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": 0,
        "indent": [
            "error",
            2
        ],
		"quotes": [
            "error",
            "single"
        ],
		"semi": [
            "error",
            "always"
        ],
},
	"globals": {
    "angular": false,
    "chai": false,
  	"describe": false,
  	"it": false,
  	"beforeEach": false,
  	"afterEach": false,
  	"before": false,
  	"after": false,
      "$": true,
      "checkAuth": true
 },
}
